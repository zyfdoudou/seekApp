.my {
  margin: 20rpx 30rpx 0rpx 30rpx;
}
.my .top-hr {
    height: 15rpx;
    background-color: #F1F1F1;
    border: 0;
    border-radius: 7rpx;
}
.my .my-top .my-top-a {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: horizontal;
    -webkit-box-direction: normal;
    -webkit-flex-direction: row;
            flex-direction: row;
    height: 120rpx;
}
.my .my-top .my-top-a .myImg {
      width: 100rpx;
      height: 100rpx;
      border-radius: 50rpx;
}
.my .my-top .my-top-a .my-top-a-p {
      padding-left: 30rpx;
      padding-top: 20rpx;
}
.my .my-top .my-top-a .my-top-a-p .my-info {
        font-size: 16rpx;
        padding-top: 10rpx;
        color: #939393;
}
.my .my-top .my-top-b {
    margin: 20rpx 30rpx 20rpx 30rpx;
}
.my .my-top .my-top-b button {
      background-color: #F0AD4E;
      border-radius: 30rpx;
}
.my .my-content .view {
    padding: 40rpx 0 20rpx 80rpx;
    height: 60rpx;
    font-size: 30rpx;
}
.my .my-content .content-img {
    padding-top: 20rpx;
    height: 60rpx;
    width: 60rpx;
    border-radius: 30rpx;
}

